{
  "name": "nextjs-docs-ja",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "npm run lint:docs",
    "lint:docs": "textlint --fix docs/**",
    "translate:diff": "tsx ./scripts/translation/diff-docs.mts -o .translation-diff.txt",
    "translate:jp": "tsx ./scripts/translation/translate.mts -l ja .translation-diff.txt",
    "translate:img": "tsx ./scripts/translation/dlimg.mts .translation-diff.txt"
  },
  "dependencies": {
    "@docusaurus/core": "3.2.0",
    "@docusaurus/module-type-aliases": "3.2.0",
    "@docusaurus/preset-classic": "3.2.0",
    "@docusaurus/theme-classic": "3.2.0",
    "@easyops-cn/docusaurus-search-local": "^0.40.1",
    "@mdx-js/react": "^3.0.1",
    "clsx": "^2.0.0",
    "docusaurus-plugin-image-zoom": "^2.0.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tsconfig/docusaurus": "^2.0.3",
    "http-server": "^14.1.1",
    "openai": "^4.62.0",
    "p-limit": "^6.1.0",
    "prettier": "3.2.5",
    "textlint": "^14.0.4",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-preset-bengo4": "^2.2.2",
    "textlint-rule-preset-ja-spacing": "^2.3.1",
    "textlint-rule-preset-ja-technical-writing": "^10.0.1",
    "textlint-rule-prh": "^6.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "zx": "^8.1.7"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

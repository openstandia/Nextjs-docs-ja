{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/docs/app-router",
      "destination": "/docs"
    },
    {
      "source": "/docs/app-router/getting-started",
      "destination": "/docs"
    },
    {
      "source": "/docs/app-router/getting-started/:path*",
      "destination": "/docs/getting-started/:path*"
    },
    {
      "source": "/docs/app-router/building-your-application/:path*",
      "destination": "/docs/app/building-your-application/:path*"
    },
    {
      "source": "/docs/app-router/api-reference/:path*",
      "destination": "/docs/app/api-reference/:path*"
    },
    {
      "source": "/docs/app-router/architecture/:path*",
      "destination": "/docs/architecture/:path*"
    },
    {
      "source": "/docs/getting-started/:path*",
      "destination": "/docs/app/getting-started/:path*"
    }
  ]
}
